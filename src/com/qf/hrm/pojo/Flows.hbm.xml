<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

        <!-- 配置hibernate -->
 <hibernate-mapping package="com.qf.hrm.pojo">
 	<class name="Flows" table="t_flows" lazy="false">
 		<id name="fId">
 			<generator class="native"></generator>
 		</id>
 		<!-- 配置普通属性 -->
 		<property name="fName"></property>
 		<property name="uId"></property>
 		<property name="time"></property>
 		<property name="context"></property>
 		<property name="state"></property>
 		<property name="sDate"></property>
 		<property name="eDate"></property>
 		<property name="reason"></property>
 		<property name="uName"></property>
 	</class>
 </hibernate-mapping>